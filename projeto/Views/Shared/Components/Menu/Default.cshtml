@model UsuarioModel



<img src="~/img/logo/logo (1).png"
     class="me-2"
     style="width: 50px; height: 50px; object-fit: contain;" />
<a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Olá @Model.Nome </a>
<button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
        aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
</button>
<div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
    <ul class="navbar-nav flex-grow-1">
        <li class="nav-item">
            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">inicio</a>
        </li>
        <li class="nav-item">
            <a class="nav-link text-dark" asp-area="" asp-controller="projetos" asp-action="Index">Projetos</a>
        </li> 

        @if (Model.Perfil == projeto.Enums.PerfilEnum.Admin)
        {
            <li class="nav-item">
                <a class="nav-link text-dark" asp-area="" asp-controller="usuario" asp-action="Index">Usuarios</a>
            </li>
        }


        
        <li class="nav-item">
            <a class="nav-link text-dark" asp-area="" asp-controller="Login" asp-action="Sair">Sair</a>
        </li>

    </ul>
</div>